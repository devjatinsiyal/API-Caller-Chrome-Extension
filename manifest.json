{
    "manifest_version": 3,
    "name": "Domain API Caller",
    "version": "1.0",
    "description": "Make an API call with the domain name as a query parameter.",
    "permissions": ["activeTab"],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
        "16": "bass-drum.png",
        "48": "bass-drum.png",
        "128": "bass-drum.png"
        }
    }
}